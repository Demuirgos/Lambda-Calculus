let operators := 
    include [functions] for {
        ++    := (n:number) => incr(n);
        ??    := (b:bool, n:number) => null(b, n);
        ==    := (n:number, m:number) => (n = m) ?? 1;
        <>    := (n:number, m:number) => (n = m) ?? 0;
        |>    := (m:number, f:number->number) => f(m)
    }
end operators